{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": ["ES2015"],
    "allowJs": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
  }
}
